<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.kumkangkind.kumkangsm2"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <!--권한설정-->
    <!--사진같은거 업로드 (외부저장소 접근)-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <!--쓰기권한-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!--인터넷-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <!--gps권한(대략)-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <!--gps권한(세부)-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <!--핸드폰정보(본인 전화번호...)-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" /> <!--아이콘 바로가기 만들기 권한-->
    <uses-permission android:name="android.permission.WAKE_LOCK"/> <!--메세지를 받기위해 핸드폰 절전상태 방지-->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/> <!--GCM메세지 권한-->
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" /><!--오레오 버전 부터 추가된  설치 권한-->
    <uses-permission android:name="android.permission.BLUETOOTH"/><!--핸드폰정보 이름 설정값을 알아오기위한 블루투스 권한-->

    <application
        android:name="com.kumkangkind.kumkangsm2.Application.ApplicationClass"
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:largeHeap="true"
        android:usesCleartextTraffic="true"
        > <!--메모리 많이쓰겠다 선언-->

        <activity android:name="com.kumkangkind.kumkangsm2.MainActivity">
            <intent-filter><!--액티비티 사이에서 주고받음-->
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/><!--LAUNCHER: app 아이콘을 눌렀을때 실행되는 액티비티-->
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name= "android.intent.category.DEFAULT"/>
                <category android:name= "android.intent.category.BROWSABLE"/>
                <data android:scheme="@string/kakao_scheme"
                      android:host="@string/kakaolink_host"/>
            </intent-filter>
        </activity>
        <activity android:name="com.kumkangkind.kumkangsm2.SearchActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ViewActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.SuListViewActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.RegisterActivity"
                  android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name="com.kumkangkind.kumkangsm2.EditTextActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.SplashView"/>
        <activity android:name="com.kumkangkind.kumkangsm2.VehicleRegisterActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.AssignUserActivity"
                  android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name="com.kumkangkind.kumkangsm2.PhotoListActivity"
                  android:configChanges="orientation|keyboardHidden|screenSize"
            />
        <activity android:name="com.kumkangkind.kumkangsm2.PItemListActivity"
                  android:configChanges="orientation|keyboardHidden|screenSize"
            />
        <activity android:name="com.kumkangkind.kumkangsm2.ActivityDownload">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="com.kumkangkind.kumkangsm2.ViewActivity2"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ActivityDailyCost2"/>
        <activity android:name="com.kumkangkind.kumkangsm2.sale.ActivitySales"/>
        <activity android:name="com.kumkangkind.kumkangsm2.sale.ActivityRegister"
                  android:windowSoftInputMode="stateAlwaysHidden"
        />
        <activity android:name="com.kumkangkind.kumkangsm2.sale.CustomerListActivity"
                  android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name="com.kumkangkind.kumkangsm2.sale.ActivityWorderListView"/>
        <activity android:name="com.kumkangkind.kumkangsm2.sale.ActivitySaleView"/>
        <activity android:name="com.kumkangkind.kumkangsm2.sqlite.ActivityMessageHistory"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ActivityMenuTest3"
            android:theme="@style/AppTheme2"/>
        <activity android:name="com.kumkangkind.kumkangsm2.LocationTreeViewActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.LocationTreeViewActivitySearch"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ProgressFloorActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.AssignmentActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.AssignmentStatusActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.DongPersonListViewActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.SuListViewActivity2"/>
        <activity android:name="com.kumkangkind.kumkangsm2.SaleOrderActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ProductionProgressActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.RegisterActivity2"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <activity android:name="com.kumkangkind.kumkangsm2.CommonActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.AddItemListActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.AddItemDetailListActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ASItemListActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ActivityDailyCostEumsung"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ComplaintImageActivity"
            android:theme="@style/Theme.ComplaintImageActivity"/>
        <activity android:name="com.kumkangkind.kumkangsm2.ProgressFloorReturnActivity"/>

        <service android:name="com.kumkangkind.kumkangsm2.fcm.MyFcmListenerService" android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <!--
        토큰 변할 때 마다 갱신을 위함, 서버에 토큰 전송
       -->
        <!-- FirebaseInstanceIdService performs security checks at runtime,
       no need for explicit permissions despite exported="true"             -->
        <service android:name="com.kumkangkind.kumkangsm2.fcm.MyInstanceIDListenerService" android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <!--
        android:exported : true면 외부에서 접근가능 false면 불가능 intent 필터가 없는 경우 기본적으로 접근 불가능 intet 필터가 있는 경우 기본적으로 true 접근 가능
        -->
        <service android:name="com.kumkangkind.kumkangsm2.fcm.RegistrationIntentService" android:exported="false"> </service>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths"/>
        </provider>


        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <meta-data android:name="com.kakao.sdk.AppKey" android:value="@string/kakao_app_key"/>

        <receiver

            android:name=".StartupReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MY_PACKAGE_REPLACED"/>
            </intent-filter>
        </receiver>
        <uses-library android:name="org.apache.http.legacy" android:required="false"/>
</application>


</manifest>
